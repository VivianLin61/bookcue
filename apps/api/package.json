{
    "name": "api",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "rimraf dist && nest build --webpack",
        "start": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "lint": "eslint --quiet \"**/*.ts\"",
        "lint:fix": "eslint \"**/*.ts\" --fix",
        "prettier": "prettier --list-different \"**/*.ts\"",
        "prettier:fix": "prettier --write \"**/*.ts\"",
        "format": "npm run lint:fix && npm run prettier:fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
    },
    "dependencies": {
        "@apollo/server": "^4.7.1",
        "@apollo/subgraph": "2.4.3",
        "@as-integrations/fastify": "^1.3.2",
        "@nestjs/apollo": "11.0.5",
        "@nestjs/common": "9.4.0",
        "@nestjs/config": "2.3.1",
        "@nestjs/core": "9.4.0",
        "@nestjs/graphql": "11.0.5",
        "@nestjs/jwt": "^10.0.3",
        "@nestjs/passport": "^9.0.3",
        "@nestjs/platform-express": "9.4.0",
        "@nestjs/terminus": "9.2.2",
        "@prisma/client": "4.14.0",
        "@vendia/serverless-express": "4.10.3",
        "aws-lambda": "1.0.7",
        "bcryptjs": "^2.4.3",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.0",
        "express": "^4.18.2",
        "graphql": "16.6.0",
        "helmet": "^7.0.0",
        "passport-jwt": "^4.0.1",
        "prisma-erd-generator": "^1.7.0",
        "prisma-nestjs-graphql": "^18.0.2",
        "reflect-metadata": "0.1.13",
        "rimraf": "5.0.0",
        "rxjs": "7.8.1"
    },
    "devDependencies": {
        "@nestjs/cli": "9.4.2",
        "@nestjs/schematics": "9.1.0",
        "@types/aws-lambda": "8.10.115",
        "@types/express": "4.17.17",
        "@types/node": "20.1.1",
        "@typescript-eslint/eslint-plugin": "5.59.5",
        "@typescript-eslint/parser": "5.59.5",
        "copy-webpack-plugin": "^11.0.0",
        "eslint": "8.40.0",
        "eslint-config-prettier": "8.8.0",
        "jest": "29.5.0",
        "prettier": "2.8.8",
        "prisma": "4.14.0",
        "serverless": "3.30.1",
        "serverless-offline": "12.0.4",
        "ts-jest": "29.1.0",
        "ts-loader": "^9.4.2",
        "ts-node": "10.9.1",
        "typescript": "5.0.4",
        "webpack": "^5.82.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "prisma": {
        "schema": "prisma/schema.prisma",
        "seed": "ts-node prisma/tools/seed.ts"
    }
}
