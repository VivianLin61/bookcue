{
  "name": "bookcue",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build:web": "pnpm exec nx build web",
    "start:web": "pnpm exec nx dev web",
    "build:api": "pnpm exec nx build api",
    "start:api": "pnpm exec nx start api",
    "start:gql:watch": "graphql-code-generator --config codegen.ts --watch",
    "----SEVERLESS SECTION----": "-------------------------",
    "sls:deplpy": "pnpm exec nx sls:deploy api",
    "sls:package": "pnpm exec nx sls:package api",
    "sls:offline": "pnpm exec nx sls:offline api",
    "sls:info": "pnpm exec nx sls:info api",
    "----DB SECTION----": "-------------------------",
    "db:generate": "pnpm --filter api exec prisma generate",
    "db:migrate:dev": "pnpm --filter api exec prisma migrate dev",
    "db:studio": "pnpm --filter api exec prisma studio",
    "db:seed": "pnpm --filter api exec prisma db seed",
    "db:all": "pnpm run db:generate && pnpm run db:migrate:dev && pnpm run db:seed",
    "----PACKAGES SECTION----": "-------------------------",
    "update:api": "pnpm --filter api update -i --latest",
    "update:web": "pnpm --filter web update -i --latest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/client-preset": "^3.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "nx": "^16.1.3"
  }
}
